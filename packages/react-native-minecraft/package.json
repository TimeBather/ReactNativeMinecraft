    "@types/react-reconciler": "^0.28.8",
    "fbjs": "^3.0.5",
    "react": "^18.3.1",
    "react-reconciler": "^0.29.2",
    "rollup": "^4.17.2"
  },
  "devDependencies": {
    "@react-native/metro-config": "^0.74.83",
    "@types/react-native": "^0.73.0",
    "esbuild": "^0.20.2",
    "metro-config": "^0.80.9",
    "react-native": "^0.74.1",
    "typescript": "^5.4.5"
