{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "rootDir": "src",
    "outDir": "lib",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictFunctionTypes": false,
    "types": []
  },
  "include": [
    "src",
  ],
}